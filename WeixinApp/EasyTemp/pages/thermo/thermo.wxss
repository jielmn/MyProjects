.upview {
  width: 100%;
  background-color: #3dc5a2;
  height: 180rpx;
  color: #a5eddf;
  font-size: 30rpx;
  padding-left:20rpx;
  padding-right:20rpx;
}

.connectview{
  display: block;
}

.connectview .left{
  display: inline-block;
  vertical-align: top;
  height: 60rpx;
  line-height: 60rpx;
}

.tempview{
  display: block;
  padding-top:20rpx;
}

.tempview .v1 {
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  vertical-align: top;  
}

.tempview .v2 {
  font-size: 60rpx;
  display: inline-block;
  vertical-align: top;
  height: 60rpx;
  line-height: 60rpx;
  padding-left: 30rpx;
  color: #fff;
}

.tempview .v3 {
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  vertical-align: top;  
  padding-left: 30rpx;
}

.tempview .v4 {
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  vertical-align: top;  
  padding-left: 30rpx;
  text-decoration:underline;
  color:#CA587A;
}

.middleview{
  width: 100%;
