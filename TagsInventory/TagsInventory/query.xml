<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window>
    <VerticalLayout bkcolor="#FF727E8C" bkcolor2="#FFFFFFFF">		
		<HorizontalLayout height="30" padding="10,5,10,5">
			<Label text="起始时间：" width="100" height="30" /><DateTime name="DateTime1" width="120" height="30" bkcolor="#FFFFFFFF"   />
		</HorizontalLayout>
		<HorizontalLayout height="30" padding="10,5,10,5">
			<Label text="结束时间：" width="100" height="30" /><DateTime name="DateTime2" width="120" height="30" bkcolor="#FFFFFFFF"   />
		</HorizontalLayout>
		<HorizontalLayout height="30" padding="10,5,10,5">
			<Label text="操作人员：" width="100" height="30" /><Edit name="operator" text="" width="120" height="30"    />
		</HorizontalLayout>
		<HorizontalLayout height="30" padding="10,5,10,5">
			<Button name="query" text="查询" width="100" height="30" normalimage="Images/btn_normal.png" hotimage="Images/btn_over.png" pushedimage="Images/btn_down.png"  />
		</HorizontalLayout>
		<HorizontalLayout padding="10,10,10,10">	
			<List name="result" bkcolor="#FFFFFFFF" inset="0,0,0,0" itemtextpadding="10,0,10,0" bordersize="1" bordercolor="#FFD7D7D7" itemshowhtml="true" vscrollbar="true" hscrollbar="false" itemalign="left"  itemaltbk="true" itemshowrowline="true" itemshowcolumnline="true" itemlinecolor="#FFD7D7D7">
				<ListHeader height="40" bkcolor="#FFE6ECF7" bordersize="1" bordercolor="#FFD7D7D7">
					<ListHeaderItem text="id" width="100" align="center" />
					<Control width="1" bkcolor="#FFD7D7D7"/>
					<ListHeaderItem text="procetype" width="100" align="center" />
					<Control width="1" bkcolor="#FFD7D7D7"/>
					<ListHeaderItem text="proceman" width="100" align="center" />
					<Control width="1" bkcolor="#FFD7D7D7"/>
					<ListHeaderItem text="procetime" width="100" align="center" />
					<Control width="1" bkcolor="#FFD7D7D7"/>
					<ListHeaderItem text="tags" width="100" align="center" />
					<Control width="1" bkcolor="#FFD7D7D7"/>
					<ListHeaderItem text="batch id" width="100" align="center" />
					<Control width="1" bkcolor="#FFD7D7D7"/>
				</ListHeader>
			</List>
        </HorizontalLayout>		
    </VerticalLayout>
</Window>