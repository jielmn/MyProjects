<?xml version="1.0" encoding="utf-8"?>
<Window>	
	<VerticalLayout>	
		<VerticalLayout inset="1,1,1,1" bordersize="1" bordercolor="#FF333333" height="102"   >		
			<TileLayout name="tilelayout" vscrollbar="true" itemsize="100,100" childpadding="10">
				<Include source="tileitem.xml" count="15"/>
			</TileLayout>		
		</VerticalLayout>
		
		<HorizontalLayout inset="10,20" height="40">
			<FileBrowse name="browse1" childvalign="center" width="400" normalimage="file='win7_button_normal.png' corner='5,5,5,5' hole='false' " hotimage="file='win7_button_hot.png' corner='5,5,5,5' hole='false' " pushedimage="file='win7_button_pushed.png' corner='5,5,5,5' hole='false' " browsewidth="50" text="浏览..."  />
			
			<Control width="10" />
			<Button name="btnShowFileName" text="获取文件名(&M)=>" width="120" normalimage="file='win7_button_normal.png' corner='5,5,5,5' hole='false' " hotimage="file='win7_button_hot.png' corner='5,5,5,5' hole='false' " pushedimage="file='win7_button_pushed.png' corner='5,5,5,5' hole='false' " shortcut="M" />
			
			<Control width="10" />
			<Edit name="edFileName" width="400" bordersize="1" bordercolor="#FF888888" />
			
		</HorizontalLayout>		
	</VerticalLayout>	
</Window>